<div class="event-area  pt--120 pb--80">
	<div class="container">
		<div class="row">
			<div class="col-md-8">
				<div class="section-title">
					<span class="text-uppercase">İLETİŞİM İŞLEMLERİ</span>
					<h2>İletişim Formu Sonuçları </h2>
				</div>
			</div>
		</div>
		<div class="row">
			@foreach (var item in ViewBag.Contact)
			{
				<div class="col-md-12">
					<div class="media align-items-center mb-5">
						<div class="media-head primary-bg">
							<p>@item.Subject</p>
						</div>
						<div class="media-body">
							<h4><a href="mailto:@item.Email">@item.Message</a></h4>
							<p><i class="fa fa-user"></i>@item.Name</p>
							<p><i class="fa fa-envelope"></i>@item.Email</p>
						</div>
					</div>
				</div>
			}
		</div><!-- row -->
	</div><!-- container -->
</div><!-- event-area -->