@foreach (var t in Model)
{
	<div class="all-teachers  pt--120 pb--70">
		<div class="container">
			<div class="row">
				<div class="col-lg-4 col-md-6">
					<div class="card mb-5">
						<img src="@t.PrePhotoURL" alt="image">
						<div class="card-body teacher-content p-25">
							<h4 class="card-title mb-1"><a href="teacher-details.html">@t.Name <b>@t.Surname</b></a></h4>
							<span class="primary-color d-block mb-4">@t.Lesson</span>
							<p class="card-text">@t.PreBio</p>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
}