<script>
    function validateInput(input) {
        // Yalnızca sayıların kalmasını sağla
        input.value = input.value.replace(/\D/g, '');

        // Maksimum 11 karakter kontrolü
        if (input.value.length > 11) {
            input.value = input.value.slice(0, 11);
        }
    }
</script>